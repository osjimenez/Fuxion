<log4net>
	<appender name="DEFAULT" type="log4net.Appender.RollingFileAppender">
		<file value="Log\Default.log" />
		<preserveLogFileNameExtension value="true"/>
		<staticLogFileName value="true" />
		<appendToFile value="true" />
		<rollingStyle value="Size" />
		<maxSizeRollBackups value="10" />
		<maximumFileSize value="10MB" />
		<layout type="log4net.Layout.XmlLayout" />
		<!--<threshold value="ALL" />-->
	</appender>
	<appender name="FILEDEFAULT" type="log4net.Appender.FileAppender">
		<immediateFlush>true</immediateFlush>
		<file value="Log\FileDefault.log" />
		<appendToFile value="true" />
		<layout type="log4net.Layout.XmlLayout" />
		<!--<threshold value="ALL" />-->
	</appender>
	<appender name="CONSOLE" type="log4net.Appender.ConsoleAppender">
		<layout type="log4net.Layout.PatternLayout">
			<conversionPattern value="%date{HH:mm:ss.fff} %9.9level - %message%newline" />
		</layout>
	</appender>
