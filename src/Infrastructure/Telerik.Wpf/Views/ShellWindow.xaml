<Window
	x:Class="Fuxion.Telerik_.Wpf.Views.ShellWindow"
	
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:viewModels="clr-namespace:Fuxion.Telerik_.Wpf.ViewModels"
	xmlns:reactiveui="http://reactiveui.net"
	xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
	mc:Ignorable="d"
	Title="Shell" Height="800" Width="1200">
	<!-- Background="{telerik:Windows11Resource ResourceKey=AlternativeBrush}"> -->
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto" />
			<RowDefinition Height="*" />
		</Grid.RowDefinitions>
		<telerik:RadMenu x:Name="Menu" />
		<telerik:RadDocking x:Name="Docking" Grid.Row="1" />
	</Grid>
</Window>
